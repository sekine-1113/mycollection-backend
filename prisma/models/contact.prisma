model Contact {
  id           String   @id @default(uuid())
  userId       Int      @map("user_id")
  reason       String
  createdAt    DateTime @default(now()) @map("created_at")
  isProcessing Boolean  @default(true) // 0: 対応中 1: 対応完了

  user User @relation(fields: [userId], references: [id])

  @@map("contact")
}
